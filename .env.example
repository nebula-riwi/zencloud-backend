# .env.example
# ‚ö†Ô∏è COPIA ESTE ARCHIVO COMO .env Y CONFIGURA TUS CREDENCIALES REALES

# üîê CONEXI√ìN PRINCIPAL POSTGRES
CONNECTION_STRING=Host=tu_host;Database=ZenCloud;Password=tu_password;Username=tu_usuario;Port=5432

# üìß CONFIGURACI√ìN SMTP
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=tu_email@gmail.com
SMTP_PASSWORD=tu_app_password
SMTP_FROMEMAIL=tu_email@gmail.com
SMTP_FROMNAME="ZenCloud"

# üê¨ MYSQL
MYSQL_HOST=mysql-ZenDb
MYSQL_PORT=3306
MYSQL_ADMIN_USER=root
MYSQL_ADMIN_PASSWORD=tu_mysql_password
MYSQL_DEFAULT_DATABASE=testdb

# üêò POSTGRESQL
POSTGRES_HOST=postgres-ZenDb
POSTGRES_PORT=5432
POSTGRES_ADMIN_USER=postgres
POSTGRES_ADMIN_PASSWORD=tu_postgres_password
POSTGRES_DEFAULT_DATABASE=testdb

# üè¢ SQL SERVER
SQLSERVER_HOST=sqlserver-ZenDb
SQLSERVER_PORT=1433
SQLSERVER_ADMIN_USER=sa
SQLSERVER_ADMIN_PASSWORD=tu_sqlserver_password
SQLSERVER_DEFAULT_DATABASE=TestDB

# üçÉ MONGODB
MONGO_HOST=mongo-ZenDb
MONGO_PORT=27017
MONGO_ADMIN_USER=mongoadmin
MONGO_ADMIN_PASSWORD=tu_mongo_password
MONGO_DEFAULT_DATABASE=testdb

# üåê CASSANDRA
CASSANDRA_HOST=cassandra-ZenDb
CASSANDRA_PORT=9042
CASSANDRA_ADMIN_USER=admin
CASSANDRA_ADMIN_PASSWORD=tu_cassandra_password
CASSANDRA_DEFAULT_KEYSPACE=testdb

# ‚ö° REDIS
REDIS_HOST=redis-ZenDb
REDIS_PORT=6379
REDIS_ADMIN_PASSWORD=tu_redis_password
